def _JugInMachine_holds(state: RawState, objects: Sequence[Object]
                         ) -> bool:
    """Determine if the jug is in the machine.
    """
    jug, machine = objects
    if state.get(jug, "is_held") > 0.5:
        return False
        
    jug_name = jug.id_name
    machine_name = machine.id_name


    attention_image = state.crop_to_objects([jug, machine])
    return state.evaluate_simple_assertion(
        f"{jug_name} is in the {machine_name}", attention_image)
    
    # dispense_pos = (0.98, 1.4, 0.2)
    # x = state.get(jug, "x")
    # y = state.get(jug, "y")
    # # On the table.
    # z = 0.2
    
    # jug_pos = (x, y, z)
    # sq_dist_to_dispense = np.sum(np.subtract(dispense_pos, jug_pos)**2)
    # return sq_dist_to_dispense < 1e-2
_JugInMachine = NSPredicate("JugInMachine", [_jug_type, _machine_type],
                                          _JugInMachine_holds)
